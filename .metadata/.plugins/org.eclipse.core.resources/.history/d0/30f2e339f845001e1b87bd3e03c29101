package questoes;

public class Ex13 {

	public static void main(String[] args) {

		int A = 11;
		int B = 20;
		
		if(A > 10) {
			
			System.out.println("A > 10");
			
			
		}if(A + B == 20) {
			
			System.out.println("A + B == 20");
			
		}if(A <= 10 && A + B != 20) {
			
			System.out.println("números não válidos");
			
		}
		
		
	}

}
